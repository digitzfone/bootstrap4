{
  "name": "bootstrap4",
  "version": "4.1.0",
  "description": "less files for bootstrap",
  "keywords": [
    "bootstrap",
    "sass"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:digitzfone/bootstrap4.git"
  },
  "bugs": {
    "url": "https://github.com/distros/bootstrap-less/issues"
  },
  "main": "js/bootstrap",
  "format": "global",
  "shim": {
    "js/bootstrap": ["jquery", "tether"],
    "js/bootstrap.min": ["jquery", "tether"]
  },
  "jspm":{
    "dependencies": {
      "jquery": "github:components/jquery",
      "tether":"github:hubspot/tether"
    }
  }
}
